      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cidade" type="string" column="CIDADE" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cep" type="string" column="CEP" length="9" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cpfcnpj" type="string" column="CPFCNPJ" length="18" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="caminhoLogo" type="string" column="CAMINHO_LOGO" length="230" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cmc" type="string" column="CMC" length="15" nullable="true">
      <options>
        <option name="fixed"/>
