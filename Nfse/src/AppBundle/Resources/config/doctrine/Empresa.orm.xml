      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="senhaPrefeitura" type="string" column="SENHA_PREFEITURA" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="producao" type="string" column="PRODUCAO" length="1" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="simples" type="string" column="SIMPLES" length="1" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="inicioAtividade" type="date" column="INICIO_ATIVIDADE" nullable="true"/>
    <field name="codCid" type="string" column="COD_CID" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
