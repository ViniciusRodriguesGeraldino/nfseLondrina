      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="eMail" type="string" column="E_MAIL" length="60" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="endereco" type="string" column="ENDERECO" length="40" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numero" type="string" column="NUMERO" length="6" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="complemento" type="string" column="COMPLEMENTO" length="35" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
