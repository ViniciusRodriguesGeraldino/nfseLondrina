<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Plano" table="plano">
    <id name="codigo" type="string" column="CODIGO" length="50">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipo" type="string" column="TIPO" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descricao" type="string" column="DESCRICAO" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ordem" type="integer" column="ORDEM" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tip" type="string" column="TIP" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contaSup" type="string" column="CONTA_SUP" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nivel" type="string" column="NIVEL" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="natureza" type="string" column="NATUREZA" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="orientacoes" type="string" column="ORIENTACOES" length="1024" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
