{% extends 'base.html.twig' %}

{% block body %}
    <h1>Servico</h1>

    <table>
        <tbody>
            <tr>
                <th>Descricao</th>
                <td>{{ servico.descricao }}</td>
            </tr>
            <tr>
                <th>Percirrf</th>
                <td>{{ servico.percIrrf }}</td>
            </tr>
            <tr>
                <th>Perccsl</th>
                <td>{{ servico.percCsl }}</td>
            </tr>
            <tr>
                <th>Percpis</th>
                <td>{{ servico.percPis }}</td>
            </tr>
            <tr>
                <th>Perccofins</th>
                <td>{{ servico.percCofins }}</td>
            </tr>
            <tr>
                <th>Perciss</th>
                <td>{{ servico.percIss }}</td>
            </tr>
            <tr>
                <th>Plano</th>
                <td>{{ servico.plano }}</td>
            </tr>
            <tr>
                <th>Isssuspenso</th>
                <td>{{ servico.issSuspenso }}</td>
            </tr>
            <tr>
                <th>Isssuspensomotivo</th>
                <td>{{ servico.issSuspensoMotivo }}</td>
            </tr>
            <tr>
                <th>Codserpref</th>
                <td>{{ servico.codSerPref }}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ servico.valor }}</td>
            </tr>
            <tr>
                <th>Ibptnac</th>
                <td>{{ servico.ibptNac }}</td>
            </tr>
            <tr>
                <th>Ibptmun</th>
                <td>{{ servico.ibptMun }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ servico.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('servico_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('servico_edit', { 'id': servico.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
